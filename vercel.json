{
  "version": 2,
  "functions": {
    "dist/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
